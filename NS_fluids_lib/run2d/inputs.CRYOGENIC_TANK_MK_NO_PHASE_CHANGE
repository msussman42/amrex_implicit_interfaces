ns.v=0
#amr.restart        = chk00400 # Name of the restart checkpoint file
#amr.trace   	    = 1
#ns.show_mem        = 1

# ----------------------------------------------------------
# -------- Inputs to physics class -------------------------
# ----------------------------------------------------------
ns.gravity          = 9.8
ns.gravity_dir      = 2 # 1..sdim
ns.invert_gravity   = 0
ns.twall            = 0.0
ns.rgasinlet        = 0.0
