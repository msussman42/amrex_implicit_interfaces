# Unimaterial slip channel flow problem
# Problem type 220
#
#                 SLIP Wall
#           ____________________
#      I -> |                  | O        |
#      N -> |           __     | U        | g(?)
#      F -> |          /  \    | T        V    
#      L -> |         |    |   | F        
#      O -> |          \__/    | L
#      W -> |                  | O
#        -> |__________________| W
#
#                 SLIP Wall
#
# laminar flow => p_in-p_out = (8 * mu * L * V_avg)/R^2  (?)


max_step  =  300000    # maximum timestep
max_step  = 10     # maximum timestep
stop_time =  1000.0  # maximum problem time

# ------------------  INPUTS TO CLASS AMR ---------------------
# set up for bubble
geometry.coord_sys      = 0        # 0 => cart, 1 => RZ
geometry.prob_lo   =  0.0 0.0
geometry.prob_hi   =  8.0 8.0  
geometry.is_periodic = 0 0
ns.temperature_weight_model=0 # 0=mass wtd avg 1=GFM 

# multigrid class
#mg.verbose = 1
#cg.verbose = 1
# set above to 2 for maximum verbosity
mg.nu_f = 40
mg.nu_0 = 1   # 1 - v-cycle 2 - w-cycle
cg.maxiter = 200
mg.bot_atol = 1.0e-13
mg.rtol_b = -0.01
#Lp.v = 1

ns.num_materials=3
ns.num_species_var=0
ns.configuration_tensor_var=0
ns.continuous_mof=0
ns.EILE_flag=-1 # -1=Weymouth and Yue 1=alternate EI and LE
ns.MOFITERMAX=15

# FSI_flag for avaiable phase/material
# 0 fluid (default)
# 1 solid (PROB_[23]D.F90)
# 2 solid (lagrangian)
# 3 ice
# 4 CTML
ns.FSI_flag=0 0 4

# Number of solid geometries
ns.CTML_FSI_numsolids=1
ns.CTML_force_model=0

# 64 x 64 for live simulations
amr.n_cell    = 64 64
# 3 for live simulations
amr.max_level = 3
# 0- 1 level 1- 2 levels  2- 3 levels
amr.regrid_int      = 1       # how often to regrid  ??
amr.n_error_buf     = 2 2 2 2 2 2  
amr.grid_eff        = 0.75 

ns.ns_tiling=false
#fabarray.mfiter_tile_size=32 32
amr.blocking_factor = 16 16 16 16
amr.space_blocking_factor=1 1 1 1
amr.time_blocking_factor=1	
ns.init_shrink    = 1.0      # scale back initial timestep
ns.enable_spectral=0
amr.check_int       = 100000  # number of timesteps between checkpoints
amr.check_file      = chk     # root name of checkpoint file
amr.plot_int        = 1
amr.plot_file       = plt 
amr.grid_log        = grdlog  # name of grid logging file
# change this if tiling vs. no tiling.
# 32 for live simulations
amr.max_grid_size   = 32
amr.file_name_digits= 7
#amr.restart         = chk4400
#amr.trace   =1

# ------------------  INPUTS TO PHYSICS CLASS -------------------
ns.dt_cutoff      = 0.000005  # level 0 timestep below which we halt

mac.mac_tol        = 1.0e-10  # was 1.0e-11 # tolerence for mac projections
mac.mac_abs_tol    = 1.0e-10  # was 1.0e-11
ns.minimum_relative_error=1.0e-22
ns.initial_project_cycles=2
ns.initial_cg_cycles=2
ns.project_solver_type=0 # 0MGPCG 1PCG 2 MINV=I
ns.projection_pressure_scale=1.0

# -2,-3 zonal tecplot, -1 plot files
ns.visual_option=-2

ns.cfl            = 0.5   # cfl number for hyperbolic system 0.5
ns.change_max     = 1.1     
ns.visc_coef      = 1.0
ns.visc_abs_tol   = 1.0e-10

# gravity directed towards xhi here.
ns.gravity        = 0.5  # abs(g)
ns.invert_gravity=1  # 0=> gravity points down (or to the left)  
                     # 1=> gravity points up (or to the right)
ns.gravity_dir=1 # x direction

ns.tension        = 0.0 0.0 0.0
ns.fixed_dt	  = 0.0002    # hardwire dt
ns.sum_interval   = 1         # timesteps between computing mass 


ns.axis_dir=0
ns.vorterr=0.0 0.0 0.0
ns.rgasinlet=1.57   # ??
ns.vinletgas=0.0    # ??
ns.twall=0.1
ns.advbot=0.0
ns.adv_vel=0.0
ns.adv_dir=2
ns.viscconst= 0.005 0.0 0.005
ns.denconst=1.0 1.0 1.0
ns.heatviscconst= 0.0 0.0 0.0
ns.solidheat_flag=0 0 0  # default (diffuse in solid)
ns.pressure_error_cutoff=0.0 0.0 0.0
ns.tempconst=293.0 293.0 293.0
ns.material_type=0 0 999  # 0 0 if INCOMP;  13 14 if COMPRESSIBLE

ns.xblob=0.0
ns.yblob=0.0
ns.zblob=0.0
ns.radblob=0.0
ns.denfact=0.0
ns.velfact=1.0  # constant inflow velocity 
ns.probtype=220 # UNIMATERIAL problem

#ns.mem_debug = 1
ns.v = 1
#ns.d = 1

# ----------------  PROBLEM DEPENDENT INPUTS
ns.lo_bc          = 1 1
ns.hi_bc          = 2 1

# >>>>>>>>>>>>>  BC FLAGS <<<<<<<<<<<<<<<<
# 0 = Interior           3 = Symmetry
# 1 = Inflow             4 = SlipWall
# 2 = Outflow            5 = NoSlipWall

# turn any of these on to generate run-time timing stats


# select single or double precision of FAB output data
#        default is whatever precision code is compiled with.
#fab.precision = FLOAT     # output in FLOAT or DOUBLE
fab.precision = DOUBLE    # output in FLOAT or DOUBLE

