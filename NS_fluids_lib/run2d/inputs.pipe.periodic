# ------------------  INPUTS TO MAIN PROGRAM  -------------------
# CHANGES FROM PAST VERSION:
# no more raster stuff (hdf), add mac.mac_abs_tol
# blob.* now becomes ns.*
#

max_step  =  999999   # maximum timestep
#max_step  =  2    # maximum timestep
stop_time =  120.0  # maximum problem time

# ------------------  INPUTS TO CLASS AMR ---------------------
# set up for bubble
geometry.coord_sys      = 0        # 0 => cart, 1 => RZ
geometry.prob_lo   =  0.0 0.0
geometry.prob_hi   =  0.4623 18.492 # .4623, 18.492 
geometry.is_periodic = 0 1          # MS 0  1 if periodic, 0  0 if not
ns.projection_order=1

# multigrid class
mg.verbose = 0
cg.verbose = 0
# set above to 2 for maximum verbosity
mg.nu_f = 40
mg.nu_0 = 1   # 1 - v-cycle 2 - w-cycle
cg.maxiter = 100  # was 200 now 100 MO
mg.bot_atol = 1.0e-8
mg.rtol_b = -0.01
Lp.v = 0

amr.n_cell    = 16 64
amr.max_level =  2  # max_level=2
# 0- 1 level 1- 2 levels  2- 3 levels
amr.regrid_int      = 1       # how often to regrid
amr.n_error_buf     = 4 4 4 4 4    # number of buffer cells in error est
amr.grid_eff        = 0.7   # what constitutes an efficient grid
# above was .55 (smaller=> less boxes)
amr.blocking_factor = 8       # block factor in grid generation
amr.check_int       = 500     # number of timesteps between checkpoints
amr.check_file      = chk     # root name of checkpoint file
amr.plot_int        = 1       
amr.plot_file       = plt 
amr.grid_log        = grdlog  # name of grid logging file
amr.max_grid_size   = 512
#amr.restart         = chk8000
#amr.trace   =1

# ------------------  INPUTS TO PHYSICS CLASS -------------------
ns.dt_cutoff      = 0.000005  # level 0 timestep below which we halt

mac.mac_abs_tol    = 1.0e-6

ns.pcav=0.0
ns.dencav=0.0
ns.pchopp=0.0
ns.denchopp=0.0
ns.soundchopp=100.0
ns.bubble_pressure=0.0
ns.bubble_density=1.0              

#amr.plotfile_on_restart=1
ns.hydrostatic_pressure=1.013
ns.visual_lo = 0 0
ns.visual_hi = 5120 5120

ns.is_viscoelastic=0
ns.elastic_time=1.0
ns.elastic_viscosity=1.0
ns.LCP_a=1.0
ns.LCP_viscosity=0.0
ns.LCP_N=0.0
ns.polymer_factor=0.0

ns.is_twophase=1
ns.is_compressible=0
ns.redist_on_init=0
ns.overlap=0.0

ns.cfl            = 0.5      # cfl number for hyperbolic system
ns.init_shrink    = 1.0      # scale back initial timestep
ns.change_max     = 1.1      # scale back initial timestep
ns.visc_coef      = 1.0
ns.gravity        = -980.0    # body force  -980.0
ns.gravityangle = 4.71238898      #for x-direction
#ns.gravityangle = 0.0
ns.tension        = 72.75
#ns.fixed_dt	  = 0.04       # hardwire dt
ns.sum_interval   = 1        # timesteps between computing mass 
ns.usekluge       = 0

ns.axis_dir=1  # axis_dir=1 standard pipe problem, =2 inflow for liquid only
               # axis_dir=3 inflow for gas only
               # set axis_dir=1 if periodic BC MO
ns.vorterr=999999.0
ns.rgasinlet=1.57
ns.vinletgas=37.5           # liquid velocity (x>x_interface) 50.0
ns.yblob5=0.0              # wall vertical liquid velocity (x=xhi)
ns.twall=0.1
ns.advbot=37.5          # gas velocity (x<x_interface)
ns.xblob5=0.0           # wall vertical gas velocity (x=xlo)
ns.adv_vel=0.0           # should always be 0.0
ns.adv_dir=2             # should always be 2
ns.viscunburn=0.01  # 0.01
ns.viscburn=0.00018
ns.viscvapor=0.00018
ns.tcenter=-1.0
ns.denspread=2.0
ns.denwater=1.0
ns.denair=0.001225
ns.denvapor=0.001225
ns.xblob=0.36984                  # base position of gas/liquid interface
#ns.yblob=10.0                # wave length of perturbation
ns.yblob=18.492                 # wave length of perturbation 18.492 
ns.zblob=0.0
#ns.radblob=0.2               # perturbation
ns.radblob=0.0                # perturbation
ns.denfact=1.0
ns.velfact=1.0e-5 # pressure gradient in z direction (streamwise direction) 
ns.probtype=41

#ns.mem_debug = 1
#ns.v = 1
#ns.d = 1

# ----------------  PROBLEM DEPENDENT INPUTS
ns.lo_bc          = 5 0  # MS 3 1 if inflow, 3 0 if periodic 
ns.hi_bc          = 5 0  # MS 5 2 if outflow, 5 0 if periodic 
ns.livepressureoverride=1

# >>>>>>>>>>>>>  BC FLAGS <<<<<<<<<<<<<<<<
# 0 = Interior           3 = Symmetry
# 1 = Inflow             4 = SlipWall
# 2 = Outflow            5 = NoSlipWall

# turn any of these on to generate run-time timing stats


# select single or double precision of FAB output data
#        default is whatever precision code is compiled with.
#fab.precision = FLOAT     # output in FLOAT or DOUBLE
fab.precision = DOUBLE    # output in FLOAT or DOUBLE

