amr.n_cell    = 256 256
# 0- 1 level 1- 2 levels  2- 3 levels
amr.max_level = 0
amr.regrid_int      = 1       # how often to regrid
amr.n_error_buf     = 2 2 2 2 2 2  
amr.grid_eff        = 0.75 

ns.face_flag=1
ns.conservative_tension_force=0

ns.ns_tiling=false
