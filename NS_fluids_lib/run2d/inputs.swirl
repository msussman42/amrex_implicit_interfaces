Lp.smooth_type=2
Lp.bottom_smooth_type=2
Lp.use_mg_precond_in_mglib=1 # def=1
mg.presmooth=2  # def=2
mg.postsmooth=2  # def=2
Lp.bottom_bottom_tol_factor=0.01 # def=0.01

ns.num_materials=2
# num_species_var=3 for diagnostics
ns.num_species_var=0
ns.MOFITERMAX=15
