     &         f_link3_esh(i,1: ns_ibm_r_esh(i)+ns_ibm_i_esh(i)))

      f_link3_esh(i,1: ns_ibm_r_esh(i)+ns_ibm_i_esh(i))=
     &f_link3_esh(i,1: ns_ibm_r_esh(i)+ns_ibm_i_esh(i))
     & +matmul(kks(1: ns_ibm_r_esh(i)+ns_ibm_i_esh(i)
