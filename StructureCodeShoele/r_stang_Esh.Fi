                   dncont=dncont+contactplane_n(i2) 
     &                          *contactplane_point(i2)
                enddo
                contactplane_n(4)=-dncont  !plane defined as ax+by+cz+d=0, here we save d

               temp_ibm2=s_function((time_alphaf-contact_time_esh(i,1))
