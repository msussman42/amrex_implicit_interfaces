        dXtar_na_esh(npg,1:3)= alphaf_Coef(ibdyg)
     &                        *target_points_v_esh(i,npg,1:3)
     &                      +timeMarchcoef(6)*dXtar_na_esh(npg,1:3)
      enddo   

      do np=1,ns_ibm_i_esh(ibdy)  
         npg=ns_ibm_r_esh(ibdy)+np
         bodypoint(1)=boundrylist_esh(ibdy,np,1)
