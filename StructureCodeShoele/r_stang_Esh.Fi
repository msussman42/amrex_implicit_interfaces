     &                     +(ibkts3-1)*IBucketnum(1)*IBucketnum(2)

                      do ibkt2_i=PBucket(i3g,ibkt),PBucket(i3g,ibkt+1)-1
                         j =sBucket_esh(i2,ibkt,ibkt2_i) 

