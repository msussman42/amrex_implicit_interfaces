             fibm1cont_esh(nea_esh(ibdy,ne,7))=
     &                            fibm1cont_esh(nea_esh(ibdy,ne,7))+
     &                                 tmpv3(1)*tmpv1x(2)
             fibm1cont_esh(nea_esh(ibdy,ne,8))=
     &                            fibm1cont_esh(nea_esh(ibdy,ne,8))+
