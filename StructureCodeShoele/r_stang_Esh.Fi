     &                     outcont(3)**2)

                     dncont=dot_product(outcont(1:3),outcont2(1:3))
                     dncont=-dncont/(abs(dncont)+eps)

                        temp_ibm2=time_alphaf  


                        if( (ksh_tmp/ddcont   .le. 10.0) 
     &                .and. (temp_ibm2 .gt. 0.05) ) then
