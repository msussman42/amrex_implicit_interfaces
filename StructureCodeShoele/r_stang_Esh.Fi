         f_link3_esh(ibdy,npg)=f_link3_esh(ibdy,bodypoint(1))
     &             +f_link3_esh(ibdy,bodypoint(2))
     &             -f_link3_esh(ibdy,bodypoint(3))       
      end do       

      f_link1_esh(i,1: ns_ibm_r_esh(i)+ns_ibm_i_esh(i))=
     & matmul(mms(1: ns_ibm_r_esh(i)+ns_ibm_i_esh(i),
