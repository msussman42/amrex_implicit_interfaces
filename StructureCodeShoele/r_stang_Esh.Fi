     &                               -gz_ibm_esh(i2,j))
     &                               /temp3_ibm

                              endif
                         endif
                     end do  !ibkt2_i
                     enddo   !ibkts1
                     enddo   !ibkts2
                     enddo   !ibkts3
                  endif
                  endif
               enddo   !i2i

!============  This part is skiped in implicit version END

             enddo !ibkt_i
