             ibktmin(3)=max(1,ibktz-1)
             ibktmax(3)=min(IBucketnum(3),ibktz+1)

             do ibkt_i=PBucket(i2g,ibkt2),PBucket(i2g,ibkt2+1)-1
                jq2=sBucket_fsh(i,ibkt2,ibkt_i,2)
