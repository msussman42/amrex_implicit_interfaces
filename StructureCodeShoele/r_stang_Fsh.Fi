           outcont(1:3)=outcont(1:3)
     &                 +(X_np1_fsh(jq,j,1:3)-X_n_fsh(jq,j,1:3))**2
       enddo
       enddo
       dxcont(1)=sqrt(dxcont(1)+dxcont(2)+dxcont(3))
       outcont(1)=sqrt(outcont(1)+outcont(2)+outcont(3))
