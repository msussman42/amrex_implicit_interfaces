              dXP1cont(1,1:3)=0.5*(DX_im_j(1:3)+DX_ip_j(1:3))
              dXP1cont(2,1:3)=0.5*(DY_i_jm(1:3)+DY_i_jp(1:3))
              call crossvec( 
