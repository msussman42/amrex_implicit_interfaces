                      do ibkt2_i=PBucket(i2g,ibkt),PBucket(i2g,ibkt+1)-1
                         jq=sBucket_fsh(i,ibkt,ibkt2_i,2)
                         j =sBucket_fsh(i,ibkt,ibkt2_i,1)   
                     if( (abs(j2-j) .gt. 8) 
