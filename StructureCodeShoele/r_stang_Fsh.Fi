     &                        *(gz_ibm_massive_fsh(i,jq2,j2)-gz(k))
     &                        /temp3_ibm
                        endif
                    
                   end do !k=1,nz 
                 end do !j=1,nx
               enddo  !i2=1,2
             end do    !j2
