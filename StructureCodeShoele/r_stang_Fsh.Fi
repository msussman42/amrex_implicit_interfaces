               i2=ibdy
               i2g=ibdyg
                    do ibkts1=ibktmin(1),ibktmax(1)
                    do ibkts2=ibktmin(2),ibktmax(2)
                    do ibkts3=ibktmin(3),ibktmax(3)
                      ibkt=
