     &                    *FabricLine_attr(i,j,iLkb0)))
     &                    /(FabricLine_attr(i,j,iLl0)
     &                    *FabricLine_attr(i,iv0,iLl0)**2
     &                    *FabricLine_attr(i,iv1,iLl0)
     &                    *(FabricLine_attr(i,j,iLl0)
     &                     + FabricLine_attr(i,iv0,iLl0)))
     &                    )

                         kkb_fbc(noi+j+ie1*noj,noi+iv1+ie1*noj)
     &                   =kkb_fbc(noi+j+ie1*noj,noi+iv1+ie1*noj)
     &                   -ksh_tmp
     &                    *(
