                if(jq .eq. FabricLine_npoint(i,i2)) then
                   iv0=FabricLine_address(i,i2,jq-1)  
                   iv1=FabricLine_address(i,i2,jq-2) 
                   iv2=FabricLine_address(i,i2,jq-3) 
                   ksh_tmp
     &              =0.5*( FabricLine_attr(i,iv0,iLl0)
