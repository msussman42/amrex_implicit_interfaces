      endif !stand alongshell/ or connected
      deallocate(
     &       aamat_fbc,kkb_fbc,mms_fbc)  
 
      elseif(Genalpha_timesolver(ibdyg) .eq. 1) then
            write(*,*)'Totally Implicit Euler is not implemented yet'
