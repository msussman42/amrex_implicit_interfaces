     &                   +Ktmp(1,ie1+1,2,ie2+1)

                         kkb_fbc(jq+ie1*noj,iv2+ie2*noi)
     &                   =kkb_fbc(jq+ie1*noj,iv2+ie2*noi)
     &                   +Ktmp(1,ie1+1,3,ie2+1)
                enddo
                enddo

               do ie2=1,3
                 do jq2=1,1
                   do ie1=1,3
                     do jq=1,1
                        Ktmp(jq,ie1,jq2,ie2)
     &                         =-FabricMesh_attr(i,j,iMs0)
     &                         /(temp_ibm1+1d-9)
