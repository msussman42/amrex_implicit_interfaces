     &                   -ksh_tmp
     &                    *(
     &                    (2*FabricLine_attr(i,iv1,iLkb0))
     &                    /(FabricLine_attr(i,iv0,iLl0)
     &                    *FabricLine_attr(i,iv1,iLl0)
     &                    *FabricLine_attr(i,iv2,iLl0)
     &                    *(FabricLine_attr(i,iv1,iLl0)
