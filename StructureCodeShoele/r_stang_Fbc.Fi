                         kkb_fbc(iv1+ie1*noi,iv0+ie2*noi)
     &                   =kkb_fbc(iv1+ie1*noi,iv0+ie2*noi)
     &                   +Ktmp(2,ie1+1,1,ie2+1)

                         kkb_fbc(iv2+ie1*noi,iv0+ie2*noi)
     &                   =kkb_fbc(iv2+ie1*noi,iv0+ie2*noi)
     &                   +Ktmp(3,ie1+1,1,ie2+1)

                         kkb_fbc(iv0+ie1*noi,iv1+ie2*noi)
     &                   =kkb_fbc(iv0+ie1*noi,iv1+ie2*noi)
