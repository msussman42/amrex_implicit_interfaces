           fibm3_fbc(i,j)=f_link3_fbc(i,j)
     &                  + dt2*(
     &                 -fibm3_fbc(i,j)    
     &                 +finer_3_ibm_fbc(i,j) 
     &                 +temp_ibm1
     &                  *fr*cos(phi_grav)
     &                 ) 
     &                 +dt2*f_impuls3_fbc(i,j)
        end do
         tmpcoord(1:3)=(/
     &                    cos(the_grav)*sin(phi_grav)
